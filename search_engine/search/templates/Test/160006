<script language=javascript>
	var current_em=1.0;
	function Povecaj()
	{
		current_em=current_em+0.1;				
		ChangeCss(".BodyContent P", "fontSize", current_em+"em");
		ChangeCss("TABLE.paket .postavka_ime", "fontSize", current_em+"em");
		ChangeCss("TABLE.paket .postavka_vsebina", "fontSize", current_em+"em");		
	}
	
	function Pomanjsaj()
	{
		current_em=current_em-0.1;
		
		ChangeCss(".BodyContent P", "fontSize", current_em+"em");
		ChangeCss("TABLE.paket .postavka_ime", "fontSize", current_em+"em");
		ChangeCss("TABLE.paket .postavka_vsebina", "fontSize", current_em+"em");		
		
	}
	
</script>





